<?xml version='1.0' encoding='utf-8'?>
<widget id="com.carex.workplace" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>carex</name>
    <description>An awesome Ionic/Cordova app.</description>
    <author email="hi@ionicframework" href="http://ionicframework.com/">Ionic Framework Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="resources/android/icon/mipmap-ldpi/ic_launcher.png" />
        <icon density="mdpi" src="resources/android/icon/mipmap-mdpi/ic_launcher.png" />
        <icon density="hdpi" src="resources/android/icon/mipmap-hdpi/ic_launcher.png" />
        <icon density="xhdpi" src="resources/android/icon/mipmap-xhdpi/ic_launcher.png" />
        <icon density="xxhdpi" src="resources/android/icon/mipmap-xxhdpi/ic_launcher.png" />
        <icon density="xxhdpi" src="resources/android/icon/mipmap-xxhdpi/ic_launcher.png" />
        <splash density="land-ldpi" src="resources/android/splash/splash.png" />
        <splash density="land-mdpi" src="resources/android/splash/drawable-mdpi/splash.png" />
        <splash density="land-hdpi" src="resources/android/splash/drawable-hdpi/splash.png" />
        <splash density="land-xhdpi" src="resources/android/splash/drawable-xhdpi/splash.png" />
        <splash density="land-xxhdpi" src="resources/android/splash/drawable-xxhdpi/splash.png" />
        <splash density="land-xxxhdpi" src="resources/android/splash/drawable-xxxhdpi/splash.png" />
        <splash density="port-ldpi" src="resources/android/splash/splash.png" />
        <splash density="port-mdpi" src="resources/android/splash/drawable-mdpi/splash.png" />
        <splash density="port-hdpi" src="resources/android/splash/drawable-hdpi/splash.png" />
        <splash density="port-xhdpi" src="resources/android/splash/drawable-xhdpi/splash.png" />
        <splash density="port-xxhdpi" src="resources/android/splash/drawable-xxhdpi/splash.png" />
        <splash density="port-xxxhdpi" src="resources/android/splash/drawable-xxxhdpi/splash.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="57" src="resources/ios/icon/Icon-App-57x57@1x.png" width="57" />
        <icon height="114" src="resources/ios/icon/Icon-App-57x57@2x.png" width="114" />
        <icon height="40" src="resources/ios/icon/Icon-App-40x40@1x.png" width="40" />
        <icon height="80" src="resources/ios/icon/Icon-App-40x40@2x.png" width="80" />
        <icon height="120" src="resources/ios/icon/Icon-App-40x40@3x.png" width="120" />
        <icon height="50" src="resources/ios/icon/Icon-Small-50x50@1x.png" width="50" />
        <icon height="100" src="resources/ios/icon/Icon-Small-50x50@2x.png" width="100" />
        <icon height="60" src="resources/ios/icon/Icon-App-60x60@1x.png" width="60" />
        <icon height="120" src="resources/ios/icon/Icon-App-60x60@2x.png" width="120" />
        <icon height="180" src="resources/ios/icon/Icon-App-60x60@3x.png" width="180" />
        <icon height="72" src="resources/ios/icon/Icon-App-72x72@1x.png" width="72" />
        <icon height="144" src="resources/ios/icon/Icon-App-72x72@2x.png" width="144" />
        <icon height="76" src="resources/ios/icon/Icon-App-76x76@1x.png" width="76" />
        <icon height="152" src="resources/ios/icon/Icon-App-76x76@2x.png" width="152" />
        <icon height="167" src="resources/ios/icon/Icon-App-83.5x83.5@2x.png" width="167" />
        <icon height="29" src="resources/ios/icon/icon.jpg" width="29" />
        <icon height="58" src="resources/ios/icon/icon.jpg" width="58" />
        <icon height="87" src="resources/ios/icon/Icon-App-29x29@3x.png" width="87" />
        <icon height="1024" src="resources/ios/icon/icon.jpg" width="1024" />
        <splash height="1136" src="resources/ios/splash/LaunchImage-568h@2x~iphone_640x1136.png" width="640" />
        <splash height="1334" src="resources/ios/splash/LaunchImage-750@2x~iphone6-landscape_1334x750.png" width="750" />
        <splash height="2208" src="resources/ios/splash/LaunchImage-1242@3x~iphone6s-landscape_2208x1242.png" width="1242" />
        <splash height="1242" src="resources/ios/splash/LaunchImage-1242@3x~iphone6s-portrait_1242x2208.png" width="2208" />
        <splash height="1536" src="resources/ios/splash/LaunchImage-Landscape@2x~ipad_2048x1536.png" width="2048" />
        <splash height="2048" src="resources/ios/splash/LaunchImage.png" width="2732" />
        <splash height="768" src="resources/ios/splash/LaunchImage-Landscape~ipad_1024x768.png" width="1024" />
        <splash height="2048" src="resources/ios/splash/LaunchImage-Portrait@2x~ipad_1536x2048.png" width="1536" />
        <splash height="2732" src="resources/ios/splash/LaunchImage.png" width="2048" />
        <splash height="1024" src="resources/ios/splash/LaunchImage-Portrait~ipad_768x1024.png" width="768" />
        <splash height="960" src="resources/ios/splash/LaunchImage-568h@2x~iphone_640x1136.png" width="640" />
        <splash height="480" src="resources/ios/splash/LaunchImage~iphone-320x480.png" width="320" />
        <splash height="2732" src="resources/ios/splash/LaunchImage.png" width="2732" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-device" spec="^2.0.1" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-ionic-webview" spec="^1.1.16" />
    <plugin name="cordova-plugin-ionic-keyboard" spec="^2.0.5" />
    <plugin name="phonegap-plugin-push" spec="^2.1.3">
        <variable name="FCM_VERSION" value="11.6.2" />
    </plugin>
    <plugin name="onesignal-cordova-plugin" spec="^2.3.3" />
    <allow-navigation href="http://172.20.15.111:8102" />
    <allow-navigation href="http://172.20.15.111:8104" />
    <engine name="ios" spec="4.5.4" />
    <engine name="android" spec="7.0.0" />
</widget>
